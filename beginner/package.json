{
  "name": "express-ts-beginner",
  "version": "1.0.0",
  "description": "A simple, beginner-friendly Express + TypeScript starter.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "npm run lint:fix"
    ]
  },
  "keywords": [
